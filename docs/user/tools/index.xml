&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span>$ dse.mstep --logger &lt;span style="color:#0000cf;font-weight:bold">2&lt;/span> --name binary_model_instance model.yaml stack.yaml signal_group.yaml
